		LocalScript,BEGIN
Description,,
ScanTime,1,False,False,
Data,string,tagname,1,"0,"
Data,string,tagval,1,"0,"
Data,string,yyyy,1,"0,"
Data,string,mm,1,"0,"
Data,string,_year,1,"0,"
Data,string,_date,1,"0,"
Data,int,i,1,"0,"
Data,string,tagname1,1,"0,"
Data,string,tagname2,1,"0,"
Data,int,k,1,"0,"
Data,int,retn,1,"0,"
Programm,BEGIN,
retn = @MessageBox("시간 동기화 하시겠습니까?", "시간 동기화", MB_YESNO);

if (retn != IDYES)
{
	return;
}
else
{
	for (i = 1; i <= 3; i = i + 1)
	{
		@sprintf(tagname, "RECOVERY.MONTHLY.LOG_DATE_TIME%02d", i);
		tagval = @GetTagValue(tagname); //00:00:00 (26/11/2021) //2021-11     2021-11-26

		if (tagval != "")
		{
			yyyy = @StringSubstring(tagval, 16, 4);
			mm = @StringSubstring(tagval, 13, 2);
		}
		else
		{
			for (k = i; k <= 3; k = k + 1)
			{
				@sprintf(tagname1, "RECOVERY.MONTHLY.YEAR%02d", k);
				@sprintf(tagname2, "RECOVERY.MONTHLY.DATE%02d", k);

				@SetTagValue(tagname1, "");
				@SetTagValue(tagname2, "");
			}
			
			@MessageBox("시간동기화 완료", "완료", MB_OK);
			return;
		}

		@sprintf(_year, "%s", yyyy);
		@sprintf(_date, "%s-%s", yyyy, mm);

		@sprintf(tagname1, "RECOVERY.MONTHLY.YEAR%02d", i);
		@sprintf(tagname2, "RECOVERY.MONTHLY.DATE%02d", i);

		@SetTagValue(tagname1, _year);
		@SetTagValue(tagname2, _date);
	}

	@MessageBox("시간동기화 완료", "완료", MB_OK);
}
Programm,END,
